{
        "ServiceName": "BezosQuoteGenerator",
        "SourceConfiguration": {
            "ImageRepository": {
                "ImageIdentifier": "257500476691.dkr.ecr.us-east-2.amazonaws.com/bezos-quote-generator:latest",
                "ImageConfiguration": {
                    "Port": "80"
                },
                "ImageRepositoryType": "ECR"
            },
            "AutoDeploymentsEnabled": true,
            "AuthenticationConfiguration": {
                "AccessRoleArn": "arn:aws:iam::257500476691:role/service-role/AppRunnerECRAccessRole"
            }
        },
        "InstanceConfiguration": {
            "Cpu": "1024",
            "Memory": "2048"
        },
        "HealthCheckConfiguration": {
            "Protocol": "HTTP",
            "Path": "/quote",
            "Interval": 10,
            "Timeout": 5,
            "HealthyThreshold": 1,
            "UnhealthyThreshold": 5
        }
}
